{
    "name": "Forward",
    "description": "Study tool for students",
    "version": "1.0.0",
    "manifest_version": 2,
    "key": "KEY FROM EXTENSION ID FOLDER",
    "icons": {
        "16": "./icons/forward16x16.png",
        "32": "./icons/forward32x32.png",
        "48": "./icons/forward48x48.png",
        "128": "./icons/forward128x128.png"
    },
    "content_security_policy": "script-src 'self'  'unsafe-eval'; object-src 'self'",
    "background": {
        "scripts": [
            "background.js",
            "jsrsasign-all-min.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "identity"
    ]
}